{
  "name": "react-native-obfuscating-transformer",
  "version": "1.0.0-0",
  "description": "Obfuscating transformer for react-native",
  "main": "dist/index.js",
  "repository": "https://github.com/ds300/react-native-obfuscating-transformer",
  "author": "David Sheldrick",
  "license": "MIT",
  "dependencies": {
    "@types/babel-core": "^6.25.3",
    "@types/node": "^9.3.0",
    "@types/semver": "^5.4.0",
    "app-root-path": "^2.0.1",
    "javascript-obfuscator": "^0.13.0",
    "jju": "^1.3.0",
    "semver": "^5.4.1",
    "source-map": "^0.6.1"
  },
  "peerDependencies": {
    "react-native": ">=0.45.0"
  },
  "scripts": {
